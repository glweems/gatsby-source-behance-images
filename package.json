    },
    "peerDependencies": {
        "gatsby": ">=2"
    },
    "devDependencies": {
        "@types/bluebird": "^3.5.27",
        "@types/node": "^12.6.8",
        "@types/request": "^2.48.2",
        "@typescript-eslint/eslint-plugin": "^1.13.0",
        "@typescript-eslint/parser": "^1.13.0",
        "eslint": "^6.1.0",
        "eslint-plugin-react": "^7.14.3",
        "typescript": "^3.5.3"
    }
